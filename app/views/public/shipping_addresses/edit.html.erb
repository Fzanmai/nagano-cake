<h2>配送先編集</h2>
<%= form_with model: @shipping_address do |f| %>
  <label for="inputPostCode">郵便番号（ハイフンなし）</label>
  <%= f.text_field :post_code, autofocus: true, id:"inputName", placeholder:"郵便番号"%>
  <label for="inputAddress">住所</label>
  <%= f.text_field :address, autofocus: true, id:"inputName", placeholder:"住所"%>
  <label for="inputName">宛名</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"宛名"%>
  <%= f.submit "編集内容を保存",class: "btn btn-success" %>
<% end %>